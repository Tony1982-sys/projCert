pipeline {
    agent any

    stages {
        stage('git checkout') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Docker build') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Docker log in') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Docker push') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
